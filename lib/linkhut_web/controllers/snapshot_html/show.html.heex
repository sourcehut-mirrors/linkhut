<.snapshot_tabs tabs={@tabs} link_id={@link.id} all_count={@all_count} request_path={@conn.request_path} />
<div class="snapshot-viewer">
  <.toolbar link={@link} snapshot={@snapshot} />
  <.details snapshot={@snapshot} link={@link} />
  <.content_iframe serve_url={@serve_url} title={@link.title} />
</div>
